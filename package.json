{
  "name": "remix-tailwind-starter",
  "version": "1.0.0",
  "description": "A Remix.run starter with tailwind 2 configured",
  "keywords": [
    "remix.run",
    "tailwind",
    "postcss"
  ],
  "repository": "mcansh/remix-tailwind-starter",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "main": "server.js",
  "scripts": {
    "build": "run-p build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:remix": "remix build2",
    "clean": "del app-build public/build",
    "vercel-build": "npm run build",
    "dev": "pm2-dev pm2.config.js",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx . --fix",
    "prepare": "husky install .config/husky",
    "typecheck": "tsc --project app"
  },
  "dependencies": {
    "@remix-run/node": "0.15.2",
    "@remix-run/react": "0.15.2",
    "@remix-run/vercel": "0.15.2",
    "cross-env": "7.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0"
  },
  "devDependencies": {
    "@mcansh/eslint-config": "3.0.1",
    "@remix-run/dev": "0.15.2",
    "@tailwindcss/jit": "0.1.18",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "autoprefixer": "10.2.5",
    "del-cli": "3.0.1",
    "eslint": "7.23.0",
    "glob": "7.1.6",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "pm2": "4.5.5",
    "postcss-cli": "8.3.1",
    "prettier": "2.2.1",
    "shell-quote": "1.7.2",
    "tailwindcss": "2.0.4",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=12"
  }
}
